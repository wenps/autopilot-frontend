{
  "name": "autopilot",
  "version": "0.1.0",
  "description": "Personal AI automation agent with browser control, VSCode integration, and multi-channel messaging",
  "keywords": ["ai", "automation", "browser", "agent", "vscode"],
  "license": "MIT",
  "author": "",
  "bin": {
    "autopilot": "dist/entry.js"
  },
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node --import tsx src/entry.ts",
    "autopilot": "node --import tsx src/entry.ts",
    "build": "tsdown",
    "check": "pnpm tsc --noEmit && pnpm lint",
    "lint": "oxlint",
    "format": "oxfmt --write",
    "format:check": "oxfmt --check",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@clack/prompts": "^1.0.1",
    "@sinclair/typebox": "^0.34.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "json5": "^2.2.3",
    "openai": "^4.80.0",
    "playwright-core": "^1.58.0",
    "undici": "^7.21.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.18.1",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "tsdown": "^0.20.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22"
  }
}
